<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xidian.smartfactoryeas.dao.SettingDao">

    <select id="querySetting">
        select type,threshold from setting
    </select>

    <update id="updateSetting">
        update setting set threshold = #{newthreshold} where type = "qiwei"
    </update>

</mapper>